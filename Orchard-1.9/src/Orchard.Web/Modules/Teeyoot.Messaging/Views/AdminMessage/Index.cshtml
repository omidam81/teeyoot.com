<div>
    @using (Script.Foot())
    {
        <script>
            function downloadTemplate(fileName) {
                $.ajax({
                    async: false,
                    cache: false,
                    type: "GET",
                    url: "@(Url.Action("Download", "AdminMessage"))",
                    data: { "fileName": fileName },
                    success: function (data) {
                    }, error: function (data, status, resp) {
                        alert('Status: ' + data.status + '. Error message: ' + data.statusText);
                    }
                });
            };
        </script>
    }


